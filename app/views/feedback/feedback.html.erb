<table class="table table-hover table-bordered">
    <thead>
      <tr>
        <th>S.no</th>
        <th>User</th>
        <th>Show</th>
        <th>Delete</th>
      </tr>
    </thead>
    <tbody>
      <% @feedback.each do |f| %>
      <tr>
          <td>1</td>
        <td><%= f.name %></td>
        <td><%= link_to(image_tag("show.png", :border=>0),{:action=>'show',:id => f.id})%></td></td>
        <td><%= link_to(image_tag("delete.gif", :border=>0),{:action => 'delete',:id => f.id})%></td>
      </tr>
      <% end %>
    </tbody>
  </table>