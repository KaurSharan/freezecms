				</div>
			</div>

			
				<div class="row">
					<!-- <div class="col-lg-12">
						<div class="col-lg-6 col-lg-offset-3 name">
							<h1>Admin Panel</h1>
						</div>
					</div> -->
					<div class="col-lg-12" id="main_content">
						<div class="col-lg-6 col-lg-offset-2">
							<%= yield %>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<footer style="height:50px;background:#3c8dbc;color:#fff;text-align:center;margin-top: 34px;">

            <div class="row">
                <div class="col-lg-12">           
                    <p>Copyright &copy; Your Freeze CMS 2016</p>
                </div>
            </div>
        </footer>
	<script type="text/javascript">$(document).ready(function() {
    if ($('textarea').length > 0) {
        var data = $('textarea');
        $.each(data, function(i) {
            CKEDITOR.replace(data[i].id);
        });
    }
});</script>
</body>
</html>