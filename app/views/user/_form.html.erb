<div class="row">
			<div class="col-lg-6">
					Enter Email:-
				</div>
				<div class="col-lg-6">
					<%= f.text_field('email',:class=>'form-control') %>
				</div>
		</div>
		<div class="row">
			<div class="col-lg-6">
					Enter Password:-
				</div>
				<div class="col-lg-6">
					<%= f.password_field('password',:class=>'form-control') %>
				</div>
		</div>
		<div class="row">
			<div class="col-lg-6">
					Is_Admin:-
				</div>
				<div class="col-lg-6">
					<%= f.check_box("is_admin") %>
				</div>
		</div>
		<div class="row">
			<div class="col-lg-6">
					Is_Active:-
				</div>
				<div class="col-lg-6">
					<%= f.check_box("is_active") %>
				</div>
		</div>