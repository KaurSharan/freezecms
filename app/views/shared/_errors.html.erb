<% if @object && @object.size > 0%>
	<div class="row">
		<h3 style="color:#ff0000">Following are the errors </h3>
	<div class="col-lg-12" style="background:#e5e5e5;color:#444;padding:2% 0%;">
		<p>
			<%	@object.each do |e|%>
			<div class="col-lg-12">
				<%=	e %>
			</div>
			<% end %>
		</p>
	</div>
	</div>
<% end %>