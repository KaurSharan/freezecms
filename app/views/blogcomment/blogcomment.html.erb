<%= form_for :blogcomment , :url=>{:action=>'check'} do |f| %>
<div class="row">
	<div class="col-lg-12">
		<%= render partial: "shared/errors", locals: {object: @object} %>
		
		<%= render partial: "form", locals: {f: f} %>
		<div class="row">
			<div class="col-lg-4 col-lg-offset-3">
							<%= submit_tag('Save User',:class=>'btn btn-primary col-lg-offset-2') %>
				</div>
		</div>
			<%= render partial: "shared/success", locals: {msg: @msg} %>
	</div>
</div>
<% end %>